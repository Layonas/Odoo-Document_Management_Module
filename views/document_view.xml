<odoo>
    <data>
        <record model="ir.ui.view" id="documnet_view_form">
            <field name="name">Documnet Form</field>
            <field name="model">document_management_module.document</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
                            <field name="company"/>
                            <field name="created_by" readonly="1"/>
                            <field name="responsible_users"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>